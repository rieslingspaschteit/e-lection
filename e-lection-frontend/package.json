{
  "name": "vite-test",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "test": "jest",
    "cy:run-local": "cypress run --e2e",
    "cy:run-ci": "cypress run --e2e --browser chrome --record --env cliJar=$CLI_JAR,isCi=true",
    "e2e": "start-server-and-test dev http://localhost:5173 cy:run-local",
    "e2e-ci": "start-server-and-test dev http://localhost:5173 cy:run-ci",
    "lint": "eslint --ext .ts,.vue ./src/ ./cypress/"
  },
  "dependencies": {
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "axios": "^1.3.0",
    "crypto-js": "^4.1.1",
    "electionguard": "^0.1.0",
    "jszip": "^3.10.1",
    "node-modules-polyfill": "^0.1.4",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.12",
    "@types/unzipper": "^0.10.5",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^12.7.0",
    "eslint": "^8.35.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.9.0",
    "jest": "^29.4.1",
    "jest-junit": "^15.0.0",
    "mysql2": "^3.2.0",
    "npm-run-all": "^4.1.5",
    "start-server-and-test": "^2.0.0",
    "ts-jest": "^29.0.5",
    "typescript": "~4.7.4",
    "unzipper": "^0.10.11",
    "vite": "^4.0.0",
    "vue-tsc": "^1.0.12"
  }
}
